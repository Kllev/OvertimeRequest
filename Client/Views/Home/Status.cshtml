@{
    Layout = "~/Views/Shared/Template/_Layout.cshtml";
}
<div class="container mt-4">
    <div class="card">
        <div class="card-header">
            <div class="row">
                <p class="h1 col-8">Requester</p>
            </div>
        </div>
        <div class="card-body">
            <table id="tableStatus" class="table table-hover table-bordered">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>RequestID</th>
                        <th>Status</th>
                        <th>RequestDate</th>
                        <th>SalaryOvertime</th>
                        <th>Action</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>
@section Scripts{
    <script src="~/js/UserRequest.js"></script>
}
